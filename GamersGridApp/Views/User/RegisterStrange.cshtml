                                                    </div>
                                                    <h6>Choose Picture</h6>
                                                </div>
                                            </div>
                                            <div class="avraam-col-sm-6">
                                                <div class="avraam-form-group">
                                                    <label>First Name <small>(required)</small></label>
                                                    @Html.TextBoxFor(v => v.FirstName, new { @class = "avraam-form-control", required = "required" })
                                                </div>
                                                <div class="avraam-form-group">
                                                    <label>Last Name <small>(required)</small></label>
                                                    @Html.TextBoxFor(v => v.LastName, new { @class = "avraam-form-control", required = "required" })
                                                </div>
                                            </div>
                                            <div class="col-sm-10 avraam-col-sm-offset-1">
